{
  "type": "module",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "scripts": {
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js, jsx, ts, tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css, scss, html, md, json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "jsdoc": "^4.0.4",
    "firebase": "^11.0.1"
  }
}
